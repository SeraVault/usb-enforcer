msgid "File size cannot be negative (use 0 for unlimited)"
msgstr "El tamaño del archivo no puede ser negativo (use 0 para ilimitado)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:80
msgid "File size limit should not exceed 10240 MB (10 GB)"
msgstr "El límite de tamaño de archivo no debe exceder 10240 MB (10 GB)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:86
msgid "Timeout should be at least 5 seconds"
msgstr "El tiempo de espera debe ser de al menos 5 segundos"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:88
msgid "Timeout should not exceed 300 seconds"
msgstr "El tiempo de espera no debe exceder los 300 segundos"

