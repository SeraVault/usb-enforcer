#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:314
msgid "Basic Enforcement"
msgstr "Application de Base"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:316
msgid "Basic Settings"
msgstr "Paramètres de Base"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:317
msgid "Core enforcement policies for USB devices"
msgstr "Politiques d'application de base pour les périphériques USB"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:320
msgid "Only Enforce on USB Devices"
msgstr "Appliquer Uniquement aux Périphériques USB"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:321
msgid ""
"When enabled, only USB devices are subject to enforcement. Other storage "
"devices (SATA, NVMe) are not affected."
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:326
msgid "Allow LUKS1 (Read-Only)"
msgstr "Autoriser LUKS1 (Lecture Seule)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:327
msgid ""
"Allow older LUKS1 encrypted devices in read-only mode. LUKS2 is recommended "
"for better security."
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:332
msgid "Allow Write with Content Scanning"
msgstr "Autoriser l'Écriture avec Analyse de Contenu"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:333
msgid ""
"Allow write access to unencrypted USB drives when content scanning is "
"enabled. Files are scanned for sensitive data before writing."
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:338
msgid "Desktop Notifications"
msgstr "Notifications de Bureau"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:339
msgid "Show desktop notifications when USB devices are connected or blocked."
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:344
msgid "Minimum Passphrase Length"
msgstr "Longueur Minimale de Phrase Secrète"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:345
msgid ""
"Minimum number of characters required for encryption passphrases. "
"Recommended: 12 or higher."
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:351
msgid "Exempted Groups"
msgstr "Groupes Exemptés"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:352
msgid ""
"Linux groups that are exempt from USB enforcement. Users in these groups can "
"use any USB devices without restrictions. Enter one group name per line."
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:357
msgid "Basic"
msgstr "Base"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:361
msgid "Security Settings"
msgstr "Paramètres de Sécurité"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:363
msgid "Access Control"
msgstr "Contrôle d'Accès"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:364
msgid "Secret token and socket security settings"
msgstr "Paramètres de sécurité des jetons secrets et des sockets"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:367
msgid "Token TTL (seconds)"
msgstr "TTL du Jeton (secondes)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:368
msgid ""
"Time-to-live for one-time tokens used in passphrase handoff. After this "
"time, tokens expire and cannot be used."
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:374
msgid "Maximum Outstanding Tokens"
msgstr "Jetons Maximums en Attente"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:375
msgid ""
"Maximum number of tokens kept in memory at once. Prevents memory exhaustion "
"from token spam."
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:380
msgid "Mount Options"
msgstr "Options de Montage"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:381
msgid "Security flags for mounting USB devices"
msgstr "Drapeaux de sécurité pour le montage de périphériques USB"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:384
msgid "Plaintext Mount Options"
msgstr "Options de Montage en Texte Clair"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:385
msgid ""
"Mount options for unencrypted USB devices. Recommended: nodev, nosuid, "
"noexec, ro"
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:390
msgid "Encrypted Mount Options"
msgstr "Options de Montage Chiffré"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:391
msgid "Mount options for encrypted USB devices. Recommended: nodev, nosuid, rw"
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:396
msgid "Require No-Execute on Plaintext"
msgstr "Exiger Non-Exécution sur Texte Clair"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:397
msgid ""
"Prevent execution of binaries from unencrypted USB devices. Recommended for "
"security."
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:401
msgid "Security"
msgstr "Sécurité"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:405
msgid "Encryption Settings"
msgstr "Échec du chiffrement"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:407
msgid "Encryption Defaults"
msgstr "Échec du chiffrement"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:408
msgid "Default settings for USB device encryption"
msgstr "Paramètres par défaut pour le chiffrement de périphériques USB"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:411
msgid "Default Encryption Type"
msgstr "Échec du chiffrement"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:412
msgid ""
"luks2: Linux Unified Key Setup (Linux only)\n"
"veracrypt: VeraCrypt (Cross-platform: Windows/Mac/Linux)\n"
"Note: VeraCrypt must be installed separately"
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:419
msgid "Encryption Target"
msgstr "Échec du chiffrement"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:420
msgid ""
"whole_disk: Encrypt entire disk\n"
"partition: Encrypt specific partition only"
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:426
msgid "Filesystem Type"
msgstr "Type de Système de Fichiers"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:427
msgid ""
"Filesystem to use after encryption:\n"
"exfat: Cross-platform (Windows/Mac/Linux)\n"
"ext4: Linux native, journaling\n"
"ntfs: Windows-focused"
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:434
msgid "Key Derivation"
msgstr "Dérivation de Clé"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:435
msgid "KDF (Key Derivation Function) settings"
msgstr "Paramètres KDF (Fonction de Dérivation de Clé)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:439
msgid "KDF Algorithm"
msgstr "Algorithme KDF"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:440
msgid ""
"argon2id: Recommended, resistant to GPU attacks\n"
"pbkdf2: Older, less secure"
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:445
msgid "Cipher Settings"
msgstr "Paramètres de Chiffrement"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:446
msgid "Encryption algorithm configuration"
msgstr "Configuration de l'algorithme de chiffrement"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:449
msgid "Cipher Algorithm"
msgstr "Algorithme de Chiffrement"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:450
msgid ""
"aes-xts-plain64: Recommended for disk encryption\n"
"aes-cbc-essiv:sha256: Older algorithm"
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:456
msgid "Key Size (bits)"
msgstr "Taille de Clé (bits)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:457
msgid ""
"512: Maximum security (recommended)\n"
"256: Standard security"
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:462
msgid "Encryption"
msgstr "Échec du chiffrement"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:468
msgid "Content Scanning (DLP)"
msgstr "Analyse de Contenu (DLP)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:469
msgid "Data Loss Prevention through real-time content scanning"
msgstr "Prévention de Perte de Données par analyse de contenu en temps réel"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:474
msgid "Enable Content Scanning"
msgstr "Activer l'Analyse de Contenu"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:475
msgid ""
"Enable real-time scanning of files for sensitive data. Prevents writing "
"credit cards, SSNs, API keys, etc. to USB devices."
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:480
msgid "Scan Encrypted Devices"
msgstr "Analyser les Périphériques Chiffrés"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:481
msgid ""
"When enabled, scanning applies to both encrypted and unencrypted devices. "
"When disabled, scanning only applies to unencrypted devices."
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:486
msgid "Action on Detection"
msgstr "Action lors de la Détection"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:487
msgid ""
"block: Prevent writing files with sensitive data\n"
"warn: Allow write but show warning\n"
"log_only: Allow write and log to journal"
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:493
msgid "Scan Categories"
msgstr "Catégories d'Analyse"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:494
msgid "Types of sensitive data to detect"
msgstr "Types de données sensibles à détecter"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:498
msgid "Enabled Categories"
msgstr "Catégories Activées"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:500
msgid "Financial (credit cards, bank accounts, SWIFT, IBAN)"
msgstr "Financier (cartes de crédit, comptes bancaires, SWIFT, IBAN)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:501
msgid "Personal (SSN, passport, driver license, phone)"
msgstr "Personnel (SSN, passeport, permis de conduire, téléphone)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:502
msgid "Authentication (API keys, passwords, tokens)"
msgstr "Authentification (clés API, mots de passe, jetons)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:503
msgid "Medical (medical records, insurance IDs)"
msgstr "Médical (dossiers médicaux, IDs d'assurance)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:508
msgid "Performance Settings"
msgstr "Paramètres de Performance"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:509
msgid "Scanning performance and limits"
msgstr "Performance et limites d'analyse"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:512
msgid "Max File Size (MB)"
msgstr "Taille Maximale de Fichier (Mo)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:513
msgid ""
"Maximum file size to scan (0 = unlimited). Larger files may be skipped or "
"sampled based on oversize_action."
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:519
msgid "Oversize File Action"
msgstr "Action Fichier Surdimensionné"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:520
msgid ""
"block: Block files exceeding max size\n"
"allow_unscanned: Allow without scanning"
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:526
msgid "Streaming Threshold (MB)"
msgstr "Seuil de Diffusion (Mo)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:527
msgid ""
"Files larger than this are written to temp disk before scanning. 0 = always "
"stream to disk."
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:533
msgid "Large File Scan Mode"
msgstr "Mode d'Analyse de Fichiers Volumineux"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:534
msgid ""
"full: Scan entire file contents\n"
"sampled: Sample portions of large files"
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:540
msgid "Scan Timeout (seconds)"
msgstr "Délai d'Analyse (secondes)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:541
msgid "Maximum time to spend scanning a single file."
msgstr "Temps maximum pour analyser un seul fichier."

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:546
msgid "Max Concurrent Scans"
msgstr "Analyses Concurrentes Maximales"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:547
msgid "Number of parallel scanning threads."
msgstr "Nombre de threads d'analyse parallèles."

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:555
msgid "Advanced Scanning"
msgstr "Analyse Avancée"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:559
msgid "Archive Scanning"
msgstr "Analyse d'Archives"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:560
msgid "Scanning files inside archives (ZIP, TAR, 7Z, RAR)"
msgstr "Analyse de fichiers dans les archives (ZIP, TAR, 7Z, RAR)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:563
msgid "Scan Archive Contents"
msgstr "Analyser le Contenu des Archives"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:564
msgid ""
"Extract and scan files inside archive files. Prevents hiding sensitive data "
"in compressed files."
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:569
msgid "Max Archive Depth"
msgstr "Profondeur Maximale d'Archive"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:570
msgid ""
"Maximum nesting level for archives (e.g., ZIP inside ZIP). Prevents zip bomb "
"attacks."
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:575
msgid "Document Scanning"
msgstr "Analyse de Documents"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:576
msgid "Scanning Office and PDF documents"
msgstr "Analyse de documents Office et PDF"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:579
msgid "Scan Documents"
msgstr "Analyser les Documents"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:580
msgid "Extract and scan text from PDF, DOCX, XLSX, PPTX, ODT files."
msgstr "Extraire et analyser le texte des fichiers PDF, DOCX, XLSX, PPTX, ODT."

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:583
msgid "Machine Learning"
msgstr "Apprentissage Automatique"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:584
msgid "Advanced pattern detection"
msgstr "Détection avancée de motifs"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:587
msgid "N-gram Analysis"
msgstr "Analyse N-gramme"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:588
msgid ""
"Use machine learning for pattern detection in unstructured text. Helps "
"detect obfuscated or formatted sensitive data."
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:592
msgid "Caching"
msgstr "Mise en Cache"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:593
msgid "Scan result caching for performance"
msgstr "Mise en cache des résultats d'analyse pour la performance"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:596
msgid "Enable Scan Cache"
msgstr "Activer le Cache d'Analyse"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:597
msgid ""
"Cache scan results based on file hash. Improves performance for repeated "
"scans of same files."
msgstr ""

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:602
msgid "Cache Size (MB)"
msgstr "Taille du Cache (Mo)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:603
msgid "Maximum size of scan result cache."
