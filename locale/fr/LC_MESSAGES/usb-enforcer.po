#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:54
msgid "Minimum passphrase length should be at least 8 characters"
msgstr "La longueur minimale de la phrase secrète doit être d'au moins 8 caractères"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:56
msgid "Maximum passphrase length should not exceed 128 characters"
msgstr "La longueur maximale de la phrase secrète ne doit pas dépasser 128 caractères"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:62
msgid "TTL should be at least 60 seconds"
msgstr "Le TTL doit être d'au moins 60 secondes"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:64
msgid "TTL should not exceed 3600 seconds (1 hour)"
msgstr "Le TTL ne doit pas dépasser 3600 secondes (1 heure)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:70
msgid "Max tokens should be at least 16"
msgstr "Les jetons maximum doivent être au moins 16"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:72
msgid "Max tokens should not exceed 1024"
msgstr "Les jetons maximum ne doivent pas dépasser 1024"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:78
msgid "File size cannot be negative (use 0 for unlimited)"
msgstr "La taille du fichier ne peut pas être négative (utilisez 0 pour illimité)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:80
msgid "File size limit should not exceed 10240 MB (10 GB)"
msgstr "La limite de taille de fichier ne doit pas dépasser 10240 Mo (10 Go)"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:86
msgid "Timeout should be at least 5 seconds"
msgstr "Le délai d'attente doit être d'au moins 5 secondes"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:88
msgid "Timeout should not exceed 300 seconds"
msgstr "Le délai d'attente ne doit pas dépasser 300 secondes"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:119
msgid "Close"
msgstr "Fermer"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:130
msgid "USB Enforcer Administration"
msgstr "Administration d'USB Enforcer"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:139
msgid "USB Enforcer Configuration"
msgstr "Configuration d'USB Enforcer"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:141
msgid "Save Configuration"
msgstr "Enregistrer la Configuration"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:147
msgid "Restart Daemon"
msgstr "Redémarrer le Démon"

#: /home/dguedry/Documents/usb-enforcer/src/usb_enforcer/ui/admin.py:148
msgid "Restart usb-enforcerd to reload configuration"
msgstr "Redémarrer usb-enforcerd pour recharger la configuration"

