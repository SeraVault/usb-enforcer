#: src/usb_enforcer/usb_enforcer_ui.py
msgid "Encrypted USB detected"
msgstr "USB chiffré détecté"

#: src/usb_enforcer/usb_enforcer_ui.py
msgid "Device {device} needs to be unlocked"
msgstr "Le périphérique {device} doit être déverrouillé"

#: src/usb_enforcer/usb_enforcer_ui.py
msgid "Unlock drive…"
msgstr "Déverrouiller le lecteur…"

#: src/usb_enforcer/usb_enforcer_ui.py
msgid "USB encryption complete"
msgstr "Chiffrement USB terminé"

#: src/usb_enforcer/usb_enforcer_ui.py
msgid "Device {device} mounted writable"
msgstr "Périphérique {device} monté en écriture"

#: src/usb_enforcer/usb_enforcer_ui.py
msgid "Encrypted USB unlocked"
msgstr "USB chiffré déverrouillé"

#: src/usb_enforcer/usb_enforcer_ui.py
msgid "Device {device} is now writable"
msgstr "Le périphérique {device} est maintenant accessible en écriture"

#: src/usb_enforcer/usb_enforcer_ui.py
msgid "Unlock failed"
msgstr "Échec du déverrouillage"

#: src/usb_enforcer/usb_enforcer_ui.py
msgid "Device {device} unlock failed"
msgstr "Échec du déverrouillage du périphérique {device}"

#: src/usb_enforcer/usb_enforcer_ui.py
msgid "Encryption failed"
msgstr "Échec du chiffrement"

#: src/usb_enforcer/usb_enforcer_ui.py
msgid "Device {device} encryption failed"
msgstr "Échec du chiffrement du périphérique {device}"

#: src/usb_enforcer/usb_enforcer_ui.py
msgid "USB mounted read-only"
msgstr "USB monté en lecture seule"

#: src/usb_enforcer/usb_enforcer_ui.py
msgid "Writing requires encryption."
