#: src/usb_enforcer/usb_enforcer_ui.py
msgid "Encrypt drive…"
msgstr "Chiffrer le lecteur…"

#: src/usb_enforcer/usb_enforcer_ui.py
msgid "⛔ File Blocked - Sensitive Data Detected"
msgstr "⛔ Fichier bloqué - Données sensibles détectées"

#: src/usb_enforcer/usb_enforcer_ui.py
msgid "File: {filepath}\\n\\nReason: {reason}\\n\\nPatterns detected: {patterns}\\n\\n{message}"
msgstr "Fichier : {filepath}\\n\\nRaison : {reason}\\n\\nMotifs détectés : {patterns}\\n\\n{message}"

#: src/usb_enforcer/usb_enforcer_ui.py
msgid "This file contains {n} sensitive pattern and cannot be written to USB."
msgid_plural "This file contains {n} sensitive patterns and cannot be written to USB."
msgstr[0] "Ce fichier contient {n} motif sensible et ne peut pas être écrit sur USB."
msgstr[1] "Ce fichier contient {n} motifs sensibles et ne peut pas être écrits sur USB."
